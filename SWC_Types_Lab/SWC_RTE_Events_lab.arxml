<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>		<!-- Learn In depth Eng.Bendary Session SWC runnables,lab2-->			<SHORT-NAME>Runnables_Lab2</SHORT-NAME>			<ELEMENTS>				<!-- Interfaces -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_Output</SHORT-NAME>					<DATA-ELEMENTS>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>SumResult</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_Input</SHORT-NAME>					<DATA-ELEMENTS>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Val1</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>						</VARIABLE-DATA-PROTOTYPE>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Val2</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!-- SWCs -->				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC1</SHORT-NAME>					<PORTS>						<P-PORT-PROTOTYPE>							<SHORT-NAME>PP_SUM</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Runnables_Lab2/SRI_Output</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>						<R-PORT-PROTOTYPE>							<SHORT-NAME>RP_SUM</SHORT-NAME>							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Runnables_Lab2/SRI_Input</REQUIRED-INTERFACE-TREF>						</R-PORT-PROTOTYPE>					</PORTS>					<INTERNAL-BEHAVIORS>						<SWC-INTERNAL-BEHAVIOR>							<SHORT-NAME>SWC1_Behavior</SHORT-NAME>							<EVENTS>							<TIMING-EVENT>								<SHORT-NAME>TimingEv_10ms</SHORT-NAME>								<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>								<!-- Note that the periond is calculated in seconds -->								<PERIOD>0.1</PERIOD>							</TIMING-EVENT>														<INIT-EVENT>								<SHORT-NAME>InitEv</SHORT-NAME>								<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>							</INIT-EVENT>														<DATA-RECEIVED-EVENT>								<SHORT-NAME>DRCEv</SHORT-NAME>								<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>								<DATA-IREF>								<!-- Note that the runnable will be triggered by the RTE event if only if Val1 received-->								<TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/SRI_Output/Val1</TARGET-DATA-ELEMENT-REF>									</DATA-IREF>							</DATA-RECEIVED-EVENT>														<DATA-SEND-COMPLETED-EVENT>								<SHORT-NAME>DSCEv</SHORT-NAME>								<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>								<!--Note that it the ARXML definintion of DSCEv differs because the data is defined in 									Application internal behaviour itsef, so source is a variable access to allow									this event to watch this variable if sent via the port successfly to trigger another event-->								<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/SWC1/SWC1_Behavior/SUM_Runnable/SumResult_Data</EVENT-SOURCE-REF>							</DATA-SEND-COMPLETED-EVENT>															</EVENTS>							<RUNNABLES>								<RUNNABLE-ENTITY>									<SHORT-NAME>SUM_Runnable</SHORT-NAME>									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>									<DATA-READ-ACCESSS>										<VARIABLE-ACCESS>											<SHORT-NAME>Val1_Data</SHORT-NAME>											<ACCESSED-VARIABLE>												<AUTOSAR-VARIABLE-IREF>													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/Runnables_Lab2/SWC1/RP_SUM</PORT-PROTOTYPE-REF>													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnables_Lab2/SRI_Input/Val1</TARGET-DATA-PROTOTYPE-REF>												</AUTOSAR-VARIABLE-IREF>											</ACCESSED-VARIABLE>										</VARIABLE-ACCESS>										<VARIABLE-ACCESS>											<SHORT-NAME>Val2_Data</SHORT-NAME>											<ACCESSED-VARIABLE>												<AUTOSAR-VARIABLE-IREF>													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/Runnables_Lab2/SWC1/RP_SUM</PORT-PROTOTYPE-REF>													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnables_Lab2/SRI_Input/Val2</TARGET-DATA-PROTOTYPE-REF>												</AUTOSAR-VARIABLE-IREF>											</ACCESSED-VARIABLE>										</VARIABLE-ACCESS>									</DATA-READ-ACCESSS>									<DATA-WRITE-ACCESSS>										<VARIABLE-ACCESS>											<SHORT-NAME>SumResult_Data</SHORT-NAME>											<ACCESSED-VARIABLE>												<AUTOSAR-VARIABLE-IREF>													<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/Runnables_Lab2/SWC1/PP_SUM</PORT-PROTOTYPE-REF>													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnables_Lab2/SRI_Output/SumResult</TARGET-DATA-PROTOTYPE-REF>												</AUTOSAR-VARIABLE-IREF>											</ACCESSED-VARIABLE>										</VARIABLE-ACCESS>									</DATA-WRITE-ACCESSS>									<SYMBOL>SUM</SYMBOL>								</RUNNABLE-ENTITY>							</RUNNABLES>						</SWC-INTERNAL-BEHAVIOR>					</INTERNAL-BEHAVIORS>				</APPLICATION-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>