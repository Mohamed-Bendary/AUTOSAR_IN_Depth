<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>			<SHORT-NAME>Composite_Lab</SHORT-NAME>			<!-- Learn In depth Eng.Bendary Session Composite SWCI-->			<ELEMENTS>				<!--Interfaces implementation -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_LightControl</SHORT-NAME>					<DATA-ELEMENTS>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>LightControl</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint8</TYPE-TREF>							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>0</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_LightActuate</SHORT-NAME>					<DATA-ELEMENTS>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>LightActuate</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint8</TYPE-TREF>							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>0</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!-- SWCs definition -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC1_SA_LightControl</SHORT-NAME>					<PORTS>						<P-PORT-PROTOTYPE>							<SHORT-NAME>Pp_LightControl</SHORT-NAME>							<!--VIP Note: you dont have to refrence the package if you are already writing in it					    but if you writing in another package, you have to refrence the package Composite_Lab 					    before /SRI_LightControl-->							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightControl</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>					</PORTS>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC2_APP_LightControlActuate_Algorithm</SHORT-NAME>					<PORTS>						<R-PORT-PROTOTYPE>							<SHORT-NAME>Rp_LightControl</SHORT-NAME>							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightControl</REQUIRED-INTERFACE-TREF>						</R-PORT-PROTOTYPE>						<P-PORT-PROTOTYPE>							<SHORT-NAME>Pp_LightActuate</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>					</PORTS>				</APPLICATION-SW-COMPONENT-TYPE>				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC3_SA_LightActuate</SHORT-NAME>					<PORTS>						<R-PORT-PROTOTYPE>							<SHORT-NAME>Rp_LightActuate</SHORT-NAME>							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</REQUIRED-INTERFACE-TREF>						</R-PORT-PROTOTYPE>					</PORTS>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>				<!--Composite definition -->				<COMPOSITION-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC4_Composite1</SHORT-NAME>					<PORTS>						<!--Defining the Delegation port & refrence it like its identical -->						<P-PORT-PROTOTYPE>							<SHORT-NAME>Rp_LightActuate_Composite</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>					</PORTS>					<COMPONENTS>						<SW-COMPONENT-PROTOTYPE>							<SHORT-NAME>SWC1Composite_SA_LightControl</SHORT-NAME>							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/SWC1_SA_LightControl</TYPE-TREF>						</SW-COMPONENT-PROTOTYPE>						<SW-COMPONENT-PROTOTYPE>							<SHORT-NAME>SWC2Composite_APP_LightControlActuate_Algorithm</SHORT-NAME>							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/SWC2_APP_LightControlActuate_Algorithm</TYPE-TREF>						</SW-COMPONENT-PROTOTYPE>					</COMPONENTS>					<CONNECTORS>						<ASSEMBLY-SW-CONNECTOR>							<SHORT-NAME>ASC_LightControl</SHORT-NAME>							<PROVIDER-IREF>								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SWC4_Composite1/SWC1Composite_SA_LightControl</CONTEXT-COMPONENT-REF>								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SWC1_SA_LightControl/Pp_LightControl</TARGET-P-PORT-REF>							</PROVIDER-IREF>							<REQUESTER-IREF>								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SWC4_Composite1/SWC2Composite_APP_LightControlActuate_Algorithm</CONTEXT-COMPONENT-REF>								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWC2_APP_LightControlActuate_Algorithm/Rp_LightControl</TARGET-R-PORT-REF>							</REQUESTER-IREF>						</ASSEMBLY-SW-CONNECTOR>						<DELEGATION-SW-CONNECTOR>							<SHORT-NAME>Del_LightActuator</SHORT-NAME>							<INNER-PORT-IREF>								<P-PORT-IN-COMPOSITION-INSTANCE-REF>									<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/SWC4_Composite1/SWC2Composite_APP_LightControlActuate_Algorithm</CONTEXT-COMPONENT-REF>									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SWC2_APP_LightControlActuate_Algorithm/Pp_LightActuate</TARGET-P-PORT-REF>								</P-PORT-IN-COMPOSITION-INSTANCE-REF>							</INNER-PORT-IREF>							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/SWC4_Composite1/Rp_LightActuate_Composite</OUTER-PORT-REF>						</DELEGATION-SW-CONNECTOR>					</CONNECTORS>				</COMPOSITION-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>