<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>			<SHORT-NAME>Interface_SRI_PRPorts</SHORT-NAME>			<!-- Learn In depth Eng.Bendary Session SWC Interfaces, SRI-->			<ELEMENTS>				<!-- Defining the SRI -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_1</SHORT-NAME>					<DATA-ELEMENTS>						<!--V1 definition -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_ObjectDistance</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>							<!-- Given an initialize value to V1 so if RTE_Write and send are used on this variable without							User initializing , this value will be given-->							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>0</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>						<!--V2 definition -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_ObjectID</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>							<!-- Given an initialize value to V2 so if RTE_Write and send are used on this variable without							User initializing , this value will be given-->							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>0</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>						<!--V3 definition -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_ObjectAngle</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>							<!-- Given an initialize value to V3 so if RTE_Write and send are used on this variable without							User initializing , this value will be given-->							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>0</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!--Defining the APP SWC to handle the LIDAR data  -->				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>APP_SWC1</SHORT-NAME>					<PORTS>						<R-PORT-PROTOTYPE>							<SHORT-NAME>Lidar_Rport</SHORT-NAME>							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interface_SRI_PRPorts/SRI_1</REQUIRED-INTERFACE-TREF>						</R-PORT-PROTOTYPE>					</PORTS>				</APPLICATION-SW-COMPONENT-TYPE>				<!--Defining the S-A SWC to handle the LIDAR data  -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>Lidar_SWC1</SHORT-NAME>					<PORTS>						<P-PORT-PROTOTYPE>							<SHORT-NAME>Lidar_Sport</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interface_SRI_PRPorts/SRI_1</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>					</PORTS>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>