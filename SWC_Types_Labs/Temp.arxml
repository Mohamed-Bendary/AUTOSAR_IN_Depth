<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>			<SHORT-NAME>Interfaces_Lab</SHORT-NAME>			<ELEMENTS>				<!-- Defining the SRI -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_1</SHORT-NAME>					<DATA-ELEMENTS>						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Variable_1</SHORT-NAME>							<SW-DATA-DEF-PROPS>								<SW-DATA-DEF-PROPS-VARIANTS>									<SW-DATA-DEF-PROPS-CONDITIONAL>										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>									</SW-DATA-DEF-PROPS-CONDITIONAL>								</SW-DATA-DEF-PROPS-VARIANTS>							</SW-DATA-DEF-PROPS>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>							<!-- Given an initialize value to V1 so if RTE_Write and send are used on this variable without							User initializing , this value will be given-->							<INIT-VALUE>							<NUMERICAL-VALUE-SPECIFICATION>							<VALUE>12</VALUE>							</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>												<!--V2 definition -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Variable_2</SHORT-NAME>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!--Defining the SWC containing the ports which use the previous SRI -->				<APPLICATION-SW-COMPONENT-TYPE>				<SHORT-NAME>SWC1</SHORT-NAME>				<PORTS>				<!--Defining a Provide port in the APP SWC -->				<P-PORT-PROTOTYPE>				<SHORT-NAME>Pport1</SHORT-NAME>				<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interfaces_Lab/SRI_1</PROVIDED-INTERFACE-TREF>				</P-PORT-PROTOTYPE>				</PORTS>								</APPLICATION-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>