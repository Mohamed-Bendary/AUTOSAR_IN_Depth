<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">

	<AR-PACKAGES>
	  <AR-PACKAGE>
	  <SHORT-NAME>SWCs_EARBS_Package</SHORT-NAME>
	  <ELEMENTS>
	  
	  <!--Defining the S-A SWC to handle the LIDAR data for the EABS project  -->
	  <SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  <SHORT-NAME>Lidar_SWC1</SHORT-NAME>
	  
	  </SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  
	  <!--Defining the S-A SWC to handle the Speed sensors data for the EABS project  -->
	  <SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  <SHORT-NAME>SpeedSensor_SWC2</SHORT-NAME>
	  
	  </SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  
	  <!--Defining the ECU abstraction SWC to connect the APP SWC with MCAL for the EABS project  -->
	  <ECU-ABSTRACTION-SW-COMPONENT-TYPE>
	  <SHORT-NAME>IOAbstraction_SWC3</SHORT-NAME>
	  
	  </ECU-ABSTRACTION-SW-COMPONENT-TYPE>
	  
	  <!--Defining the App SWC for EABS project algorithm -->
	  <APPLICATION-SW-COMPONENT-TYPE>
	  <SHORT-NAME>EABS_SWC4</SHORT-NAME>
	  
	  </APPLICATION-SW-COMPONENT-TYPE>
	  
	   <!--Defining the parameter SWC for the calibration data of the distances for the EABS project  -->
	  <PARAMETER-SW-COMPONENT-TYPE>
	  <SHORT-NAME>Parameters_SWC5</SHORT-NAME>
	  
	  </PARAMETER-SW-COMPONENT-TYPE>
	  
	  <!--Defining the S-A SWC to handle the LED warnings in d3 case for the EABS project  -->
	  <SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  <SHORT-NAME>LED_Warning_SWC6</SHORT-NAME>
	  
	  </SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
	  
	  <!--Defining the Complex Drivers SWC to handle the Breaks (We may need undefined driver to handle the relays ASAP) 
		  in d1 case for the EABS project  -->
	  <COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>
	  <SHORT-NAME>Brakes_CDD_SWC7</SHORT-NAME>
	  
	  </COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>
	  
	  <SERVICE-SW-COMPONENT-TYPE>
	  <SHORT-NAME>DIgManger_SWC8</SHORT-NAME>
	  
	  </SERVICE-SW-COMPONENT-TYPE>
	  
	  
	  <!-- Defining the NVB SWC to gandle the commm with the NV manager -->
	  <NV-BLOCK-SW-COMPONENT-TYPE>
	  <SHORT-NAME>Flash_NVBLCK_SWC9</SHORT-NAME>
	  
	  </NV-BLOCK-SW-COMPONENT-TYPE>
	  
	  
	  
	  
	  
	  </ELEMENTS>
	  
	  
	  
	  
	  </AR-PACKAGE>
	
	
	
	
	
	
	
	
	</AR-PACKAGES>
	
	
	
</AUTOSAR>
