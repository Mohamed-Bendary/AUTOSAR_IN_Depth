<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///D:/Embedded%20courses/AUTOSAR_IN_DEPTH/Common/AUTOSAR_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>			<SHORT-NAME>Runnable_lab</SHORT-NAME>			<!-- Learn In depth Eng.Bendary Session SWC runnables,lab1-->			<ELEMENTS>				<!-- Defining the SRI -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI_Throttle</SHORT-NAME>					<DATA-ELEMENTS>						<!--V1 definition -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>CMD_Throttle</SHORT-NAME>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!--Defiing the SWC which contain the algorithm of throttle CMD calculation -->				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>ThrotleControlSWC</SHORT-NAME>					<PORTS>						<P-PORT-PROTOTYPE>							<SHORT-NAME>PP_SRI_ThrotleCMD</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRI_Throttle</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>					</PORTS>					<INTERNAL-BEHAVIORS>						<SWC-INTERNAL-BEHAVIOR>							<SHORT-NAME>ThControl_Behaviuor</SHORT-NAME>							<RUNNABLES>								<RUNNABLE-ENTITY>									<SHORT-NAME>Set_ThCMD_Runnnable</SHORT-NAME>									<SYMBOL>Set_ThCMD</SYMBOL>								</RUNNABLE-ENTITY>							</RUNNABLES>						</SWC-INTERNAL-BEHAVIOR>					</INTERNAL-BEHAVIORS>				</APPLICATION-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>